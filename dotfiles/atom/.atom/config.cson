"*":
  "atom-beautify":
    elm:
      beautify_on_save: true
    general:
      _analyticsUserId: "c5bea873-1386-4b4f-9a31-85b7f0573f4b"
    haskell:
      beautify_on_save: true
  "atom-material-ui":
    colors:
      abaseColor: "#325f75"
      accentColor: "#9a4c26"
      genAccent: true
      predefinedColor: "Blue Grey"
    tabs:
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui:
      panelContrast: true
  core:
    autoHideMenuBar: true
    closeDeletedFileTabs: true
    destroyEmptyPanes: false
    disabledPackages: [
      "language-gfm"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "atom-dark-syntax"
    ]
  editor:
    atomicSoftTabs: false
    fontFamily: "Fira Code"
    fontSize: 13
    preferredLineLength: 100
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 2
  "exception-reporting":
    userId: "f96e777f-ace7-9fe4-3f87-ac9f33d8e474"
  "haskell-ghc-mod":
    useLinter: true
  "ide-haskell":
    onSavePrettify: true
  latex:
    enableShellEscape: true
    loggingLevel: "error"
    opener: "pdf-view"
  "linter-elm-make":
    autoscrollIssueIntoView: true
    showInferredTypeAnnotations: true
  "linter-ui-default":
    showPanel: true
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
    ]
  tabs:
    addNewTabsAtEnd: true
    enableVcsColoring: true
  welcome:
    showOnStartup: false
".bibtex.text":
  editor:
    softWrapAtPreferredLineLength: false
".gfm.source":
  editor:
    preferredLineLength: 80
".latex.log.text":
  editor:
    softWrapAtPreferredLineLength: false
".latex.tex.text":
  editor:
    softWrapAtPreferredLineLength: false
".md.text":
  editor:
    preferredLineLength: 80
".tex.text":
  editor:
    softWrapAtPreferredLineLength: false
