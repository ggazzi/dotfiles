
#!/usr/bin/env bash
#
# Summary: Open the GitHub page for a project
#
# Usage: {cmd} <project>

set -e

# TODO: extract the code to cd into existing project

# Provide completions
if [ "$1" = "--complete" ]; then
  dev pj list
  exit 0
fi

project="$1"

# Change into project directory
cd "$WORKSPACE/$project"

# Open the GitHub page
gh repo view --web
