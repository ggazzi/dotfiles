// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+right",
    "command": "cursorWordPartRight", "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+right",
    "command": "cursorWordPartRightSelect", "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "cursorWordEndRight", "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+shift+right",
    "command": "cursorWordEndRightSelect", "when": "textInputFocus"
  },
  {
    "key": "ctrl+left",
    "command": "cursorWordPartStartLeft", "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+left",
    "command": "cursorWordPartStartLeftSelect", "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "cursorWordStartLeft", "when": "textInputFocus"
  },
  {
    "key": "ctrl+alt+shift+left",
    "command": "cursorWordStartLeftSelect", "when": "textInputFocus"
  },
  {
    "key": "ctrl+backspace",
    "command": "deleteWordPartLeft", "when": "textInputFocus && !editorReadonly"
    
  },
  {
    "key": "ctrl+alt+backspace",
    "command": "deleteWordLeft", "when": "textInputFocus && !editorReadonly"
    
  },
  {
    "key": "ctrl+delete",
    "command": "deleteWordPartRight", "when": "textInputFocus && !editorReadonly"
    
  },
  {
    "key": "ctrl+alt+delete",
    "command": "deleteWordRight", "when": "textInputFocus && !editorReadonly"
    
  }
]